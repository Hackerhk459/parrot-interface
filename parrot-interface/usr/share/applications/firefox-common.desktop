#!/usr/bin/env xdg-open
[Desktop Entry]
Encoding=UTF-8
Name=Firefox
Exec=firefox %u
Terminal=false
X-MultipleArgs=false
Type=Application
Icon=iceweasel
Categories=Network;WebBrowser;
